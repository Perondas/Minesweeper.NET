<Window x:Class="Minesweeper.View.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minesweeper.View.Windows"
        x:Name="This"
        mc:Ignorable="d"
        Title="Minesweeper" Height="450" Width="800">
    <Grid>
        <ItemsControl ItemsSource="{Binding ElementName=This, Path=GameVm.Board.Cells}" ItemTemplateSelector="{StaticResource CellTemplateSelector}">
            
        </ItemsControl>
    </Grid>
</Window>
